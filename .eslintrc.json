{
  "parser":  "@typescript-eslint/parser",
  "extends": [
    "@cognite"
  ],
  "rules": {
    "@typescript-eslint/indent": "off",
    "@typescript-eslint/no-var-requires": "off",
    "@typescript-eslint/no-unused-vars": ["error", {
      "vars": "all",
      "args": "after-used",
      "ignoreRestSiblings": true
    }],
    "react/no-array-index-key": "warn"
  }
}
