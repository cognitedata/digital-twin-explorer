		"build": "react-app-rewired build --scripts-version @cognite/react-scripts",
		"test": "react-app-rewired test --env=jsdom  --scripts-version @cognite/react-scripts",
		"eject": "react-app-rewired eject --scripts-version @cognite/react-scripts",
		"format": "prettier --write '**/*.{js,jsx,json,md}'",
